generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name  String?
  email String?
  cpf Int @unique 
  password String

  @@map("user")
}

model Cars {
  id Int  @id @default(autoincrement())
  car_model String
  car_brand String
  car_price Float?

  @@map("cars")
}

model CarEnjoy {
  id Int @id @default(autoincrement())
  id_user Int
  id_car Int

  @@map("car_enjoy")
}